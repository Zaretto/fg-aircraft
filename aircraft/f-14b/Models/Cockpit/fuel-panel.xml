<?xml version="1.0"?>



<PropertyList>
	<path>fuel-panel.ac</path>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>feed-guard-lever</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/f-14b/controls/fuel/feed-guard-lever</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>feed-guard-lever</object-name>
		<property>sim/model/f-14b/controls/fuel/feed-guard-lever</property>
		<factor>85</factor>
		<center>
			<x-m>-0.0082</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0860</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle-horiz-R</object-name>
		<property>fdm/jsbsim/fcs/right-elevator-pos-deg</property>
		<interpolation>
			<entry><ind>-35</ind><dep>90</dep></entry>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>15</ind><dep>-40</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0011</x-m>
			<y-m>-0.0438</y-m>
			<z-m> 0.0547</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>needle-horiz-L</object-name>
		<property>fdm/jsbsim/fcs/left-elevator-pos-deg</property>
		<interpolation>
			<entry><ind>-35</ind><dep>90</dep></entry>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>15</ind><dep>-40</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0011</x-m>
			<y-m>-0.0438</y-m>
			<z-m> 0.0547</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle-rudder-R</object-name>
		<property>fdm/jsbsim/fcs/rudder-pos-deg</property>
		<interpolation>
			<entry><ind>-30</ind><dep>55</dep></entry>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>30</ind><dep>-55</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0011</x-m>
			<y-m>-0.0068</y-m>
			<z-m> 0.0713</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>needle-rudder-L</object-name>
		<property>fdm/jsbsim/fcs/rudder-pos-deg</property>
		<interpolation>
			<entry><ind>-30</ind><dep>55</dep></entry>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>30</ind><dep>-55</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0011</x-m>
			<y-m>-0.0068</y-m>
			<z-m> 0.0713</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>


	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>qty-sel-switch-up</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/f-14b/controls/fuel/qty-sel-switch</property>
				<step>1</step>
				<max>1</max>
				<wrap>fase</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>qty-sel-switch-down</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/f-14b/controls/fuel/qty-sel-switch</property>
				<step>-1</step>
				<min>-1</min>
				<wrap>fase</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>qty-sel-switch</object-name>
		<property>sim/model/f-14b/controls/fuel/qty-sel-switch</property>
		<factor>20</factor>
		<center>
			<x-m>-0.0014</x-m>
			<y-m>0.0000</y-m>
			<z-m>0.0000</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>dump-switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>f14.fuel_dump_switch_toggle()</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>dump-switch</object-name>
		<property>sim/model/f-14b/controls/fuel/dump-switch</property>
		<factor>45</factor>
		<center>
			<x-m>0.0020</x-m>
			<y-m>0.0000</y-m>
			<z-m>0.0032</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation> 
		<type>select</type>
		<object-name>left-spoilers-off</object-name>
		<condition>
			<equals>
				<property>surface-positions/left-spoilers</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>
	<animation> 
		<type>select</type>
		<object-name>right-spoilers-off</object-name>
		<condition>
			<equals>
				<property>surface-positions/right-spoilers</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>

<!-- left spoilers -->
	<animation> 
		<type>select</type>
		<object-name>sp-L-ext-down</object-name>
		<condition>
			<less-than>
				<property>surface-positions/left-spoilers</property>
				<value>0</value>
			</less-than>
		</condition>
	</animation>
	<animation> 
		<type>select</type>
		<object-name>sp-L-ext-up</object-name>
		<condition>
			<greater-than>
				<property>surface-positions/left-spoilers</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation> 
		<type>select</type>
		<object-name>sp-L-int-down</object-name>
		<condition>
			<less-than>
				<property>surface-positions/inner-left-spoilers</property>
				<value>0</value>
			</less-than>
		</condition>
	</animation>
	<animation> 
		<type>select</type>
		<object-name>sp-L-int-up</object-name>
		<condition>
			<greater-than>
				<property>surface-positions/inner-left-spoilers</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
<!-- right spoilers -->
	<animation> 
		<type>select</type>
		<object-name>sp-R-ext-down</object-name>
		<condition>
			<less-than>
				<property>surface-positions/right-spoilers</property>
				<value>0</value>
			</less-than>
		</condition>
	</animation>
	<animation> 
		<type>select</type>
		<object-name>sp-R-ext-up</object-name>
		<condition>
			<greater-than>
				<property>surface-positions/right-spoilers</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation> 
		<type>select</type>
		<object-name>sp-R-int-down</object-name>
		<condition>
			<less-than>
				<property>surface-positions/inner-right-spoilers</property>
				<value>0</value>
			</less-than>
		</condition>
	</animation>
	<animation> 
		<type>select</type>
		<object-name>sp-R-int-up</object-name>
		<condition>
			<greater-than>
				<property>surface-positions/inner-right-spoilers</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>




	<!---->
	<animation>
		<type>rotate</type>
		<object-name>refuel-probe-switch</object-name>
		<property>sim/model/f-14b/controls/fuel/refuel-probe-switch</property>
		<factor>20</factor>
		<center>
			<x-m>0.0014</x-m>
			<y-m>0.0000</y-m>
			<z-m>-0.0021</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- AntiSkid / Spoiler braking -->
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>hotspot-spoiler-brake-both</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>f14.set_spoiler_brake(2)</script>
			</binding>
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>hotspot-spoiler-brake-off</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>f14.set_spoiler_brake(0)</script>
			</binding>
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>hotspot-spoiler-brake-spoilers</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>f14.set_spoiler_brake(1)</script>
			</binding>
		</action>
	</animation>

	<animation>
		<object-name>antiskid-spoiler-switch</object-name>
		<type>rotate</type>
		<property>controls/flight/ground-spoilers-armed</property>
		<interpolation>
			<entry><ind>1</ind><dep>45</dep></entry>
			<entry><ind>0</ind><dep>0</dep></entry>
		</interpolation>
		<center>
			<x-m>  0.0036 </x-m>
			<y-m>  0.0239 </y-m>
			<z-m> -0.0502 </z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>



	<!-- lighting -->
  <animation>
    <object-name>mount</object-name>
      <object-name>switches</object-name>
      <type>material</type>
    <condition>
      <greater-than>
        <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
        <value>0</value>
      </greater-than>
      <not>
        <property>sim/rendering/rembrandt/enabled</property>
      </not>
    </condition>
    <emission>
      <factor-prop>controls/lighting/dome-norm</factor-prop>
      <red>  1.000</red>
      <green>0.576</green>
      <blue> 0.161</blue>
      <red-prop>controls/lighting/dome-red</red-prop>
      <green-prop>controls/lighting/dome-green</green-prop>
      <blue-prop>controls/lighting/dome-blue</blue-prop>
    </emission>
  </animation>

  <!-- implicit ALS lighting see http://wiki.flightgear.org/ALS_technical_notes -->
  <effect>
    <name>f14panel</name>
    <inherits-from>Effects/f14panel</inherits-from>
    <object-name>mount</object-name>
  </effect>
  <effect>
    <name>f14panel-knob</name>
    <inherits-from>Effects/f14panel-knob</inherits-from>
  </effect>


    <animation>
        <object-name>instrument</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <greater-than-equals>
                <property>controls/lighting/instruments-norm</property>
                <property>controls/lighting/dome-norm</property>
            </greater-than-equals>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/instruments-norm</factor-prop>
            <red>  1.1000</red>
            <green>0.576</green>
            <blue> 0.161</blue>
            <red-prop>controls/lighting/instrument-red</red-prop>
            <green-prop>controls/lighting/instrument-green</green-prop>
            <blue-prop>controls/lighting/instrument-blue</blue-prop>

        </emission>
    </animation>
    <animation>
        <object-name>instrument</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <greater-than>
                <property>controls/lighting/dome-norm</property>
                <property>controls/lighting/instruments-norm</property>
            </greater-than>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/dome-norm</factor-prop>
            <red>  1.1000</red>
            <green>0.576</green>
            <blue> 0.161</blue>
            <red-prop>controls/lighting/instrument-red</red-prop>
            <green-prop>controls/lighting/instrument-green</green-prop>
            <blue-prop>controls/lighting/instrument-blue</blue-prop>
        </emission>
    </animation>

</PropertyList>
