<?xml version="1.0"?>

<PropertyList>

	<sim>

		<description>Grumman F-14B </description>
		<author>Enrique Laso - Alexis Bory</author>
		<status>early production</status>
		<rating>
			<FDM type="int">3</FDM>
			<systems type="int">3</systems>
