				<signal><type>float</type><property type="string">sim/multiplay/generic/float[2]</property></signal>
				<!-- Slats -->
				<signal><type>float</type><property type="string">sim/multiplay/generic/float[3]</property></signal>
				<!-- Wing Bend -->
				<signal><type>float</type><property type="string">surface-positions/wing-fold-pos-norm</property></signal>
				<!-- Wing Fold -->
				<signal><type>float</type><property type="string">surface-positions/wing-pos-norm</property></signal>
				<!-- Tail Hook -->
				<signal><type>float</type><property type="string">gear/tailhook/position-norm</property></signal>
				<!-- Stabilisator L -->
				<signal><type>float</type><property type="string">sim/multiplay/generic/float[4]</property></signal>
				<!-- Stabilisator R -->
				<signal><type>float</type><property type="string">sim/multiplay/generic/float[5]</property></signal>
			</config>
		</flight-recorder>


