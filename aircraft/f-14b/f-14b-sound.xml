<?xml version="1.0"?>
 
<PropertyList>

	<fx>
		<!-- Engines -->
		<engine-jet>
			<name>engine-jet</name>
			<path>Sounds/jet.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>-180.0</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[0]/n1</property>
				<ofset>0.1</ofset>
				<factor>.001</factor>
			</pitch>
			<volume>
				<property>engines/engine[0]/thrust-lbs</property>
				<factor>0.000025</factor>
				<min>0.2</min>
				<max>0.75</max>
			</volume>
		</engine-jet>
		<engine-rumble>
			<name>engine-rumble</name>
			<path>Sounds/rumble.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
			</condition>
			<property>engines/engine[0]/afterburner</property>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>-180.0</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[0]/afterburner</property>
				<factor>1</factor>
				<offset>0.3</offset>
			</pitch>
			<volume>
				<property>engines/engine[0]/afterburner</property>
				<!--<factor>0.7</factor>-->
				<factor>1.2</factor>
				<offset>0.15</offset>
				<!--<max>1</max>-->
			</volume>
		</engine-rumble>
		<engine-whine-external>
			<name>external-whine</name>
			<path>Sounds/turbojet_fan.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>1</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.5</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[0]/n1</property>
				<ofset>0.7</ofset>
				<factor>.003</factor>
			</pitch>
			<volume>
				<property>engines/engine/n1</property>
				<offset>1.0</offset>
				<factor>-0.008</factor>
				<max>0.6</max>
			</volume>
		</engine-whine-external>
		<engine-whine-internal>
			<name>internal-whine</name>
			<path>Sounds/whine.wav</path>
			<mode>looped</mode>
			<condition>
				<property>sim/current-view/internal</property>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
			</condition>
			<pitch>
				<property>engines/engine[0]/n1</property>
				<ofset>0.1</ofset>
				<factor>.0016</factor>
			</pitch>
			<volume>
				<property>engines/engine[0]/thrust-lbs</property>
				<factor>0.000012</factor>
				<min>0.2</min>
				<max>0.7</max>
			</volume>
		</engine-whine-internal>
		<engine-jet-internal>
			<name>internal-jet</name>
			<path>Sounds/jet.wav</path>
			<mode>looped</mode>
			<condition>
				<property>sim/current-view/internal</property>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
			</condition>
			<pitch>
				<property>engines/engine[0]/n1</property>
				<ofset>0.1</ofset>
				<factor>.0016</factor>
			</pitch>
			<volume>
				<property>engines/engine[0]/thrust-lbs</property>
				<factor>0.00005</factor>
				<min>0.1</min>
				<max>0.9</max>
			</volume>
		</engine-jet-internal>

		<!-- Rumble -->
		<rumble>
			<name>rumble</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<or>
					<property>gear/gear[0]/wow</property>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
				</or>
			</condition>
			<volume>
				<property>velocities/groundspeed-kt</property>
				<factor>0.01</factor>
				<max>1</max>
			</volume>
		</rumble>

		<!-- Gears -->
		<gear-0-transit>
			<name>gear-0-transit</name>
			<mode>in-transit</mode>
			<path>Sounds/gear.wav</path>
			<property>gear/gear[0]/position-norm</property>
			<!--<pitch>
				<property>gear/gear[0]/position-norm</property>
				<offset>0.2</offset>
				<factor>0.1</factor>
			</pitch>-->
			<volume><factor>1.1</factor></volume>
		</gear-0-transit>
		<gear-0-lock>
			<name>gear-0-lock</name>
			<path>Sounds/gear-lck.wav</path>
			<condition>
				<or>
					<equals>
						<property>gear/gear[0]/position-norm</property>
						<value>0</value>
					</equals>
					<equals>
						<property>gear/gear[0]/position-norm</property>
						<value>0</value>
					</equals>
				</or>
			</condition>
			<volume><factor>0.7</factor></volume>
		</gear-0-lock>


		<!-- wind -->
		<wind>
			<name>wind</name>
			<mode>looped</mode>
			<path>Aircraft/f-14b/Sounds/wind.wav</path>
			<condition>
				<equals>
					<property>sim/current-view/view-number</property>
					<value>0</value>
				</equals>
				<greater-than>
					<property>canopy/position-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
		</wind>

		<!-- Air-brakes -->
		<air-brakes>
			<name>air-brakes</name>
			<mode>looped</mode>
			<path>Aircraft/f-14b/Sounds/air-brakes.wav</path>
			<condition>
				<equals>
					<property>sim/current-view/view-number</property>
					<value>0</value>
				</equals>
				<greater-than>
					<property>surface-positions/speedbrake-pos-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
			<volume>
				<property>surface-positions/speedbrake-pos-norm</property>
				<factor>1.5</factor>
			</volume>
		</air-brakes>


		<!-- Squeal -->
		<squeal>
			<name>squeal</name>
			<path>Sounds/squeal.wav</path>
			<condition>
				<property>gear/gear[0]/wow</property>
			</condition>
			<volume>
				<property>velocities/speed-down-fps</property>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>velocities/groundspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>velocities/groundspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
		</squeal>
		<squeal>
			<name>squeal</name>
			<path>Sounds/squeal.wav</path>
			<condition>
				<property>gear/gear[1]/wow</property>
			</condition>
			<volume>
				<property>velocities/speed-down-fps</property>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>velocities/groundspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
		</squeal>
		<squeal>
			<name>squeal</name>
			<path>Sounds/squeal.wav</path>
			<condition>
				<property>gear/gear[2]/wow</property>
			</condition>
			<volume>
				<property>velocities/speed-down-fps</property>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>velocities/groundspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>velocities/groundspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
		</squeal>

		<flap-in-transit>
			<name>flaps-in-transit</name>
			<mode>in-transit</mode>
			<path>Sounds/flaps.wav</path>
			<property>surface-positions/flap-pos-norm</property>
			<pitch>
				<property>surface-positions/flap-pos-norm</property>
				<factor>-0.1</factor>
			</pitch>
			<volume><factor>0.4</factor></volume>
		</flap-in-transit>

		<canopy>
			<name>canopy</name>
			<mode>in-transit</mode>
			<path>Aircraft/f-14b/Sounds/canopy-trans.wav</path>
			<property>canopy/position-norm</property>
			<volume><factor>0.2</factor></volume>
		</canopy>

		<canopy-close>
			<name>canopy-close2</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/canopy-close2.wav</path>
			<condition>
				<less-than-equals>
					<property>canopy/position-norm</property>
					<value>0</value>
				</less-than-equals>
			</condition>
			<volume><factor>0.3</factor></volume>
		</canopy-close>

		<!-- stall warnings -->
		<pre-stall>
			<name>pre-stall</name>
			<mode>looped</mode>
			<path>Aircraft/f-14b/Sounds/stall-600-chopped.wav</path>
			<condition>
				<greater-than>
					<property>orientation/alpha-deg</property>
					<value>18</value>
				</greater-than>
				<less-than-equals>
					<property>orientation/alpha-deg</property>
					<value>22.5</value>
				</less-than-equals>
				<greater-than>
					<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
					<value>10</value>
				</greater-than>
				<or>
					<equals>
						<property>sim/current-view/name</property>
						<value>Cockpit View</value>
					</equals>
					<equals>
						<property>sim/current-view/name</property>
						<value>RIO View</value>
					</equals>
				</or>
			</condition>
			<volume><factor>0.4</factor></volume>
		</pre-stall>


		<stall>
			<name>stall</name>
			<mode>looped</mode>
			<path>Aircraft/f-14b/Sounds/stall-600-continue.wav</path>
			<condition>
				<greater-than>
					<property>orientation/alpha-deg</property>
					<value>22.5</value>
				</greater-than>
				<greater-than>
					<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
					<value>10</value>
				</greater-than>
				<or>
					<equals>
						<property>sim/current-view/name</property>
						<value>Cockpit View</value>
					</equals>
					<equals>
						<property>sim/current-view/name</property>
						<value>RIO View</value>
					</equals>
				</or>
			</condition>
			<volume><factor>0.6</factor></volume>
		</stall>

		<!-- Weapons -->
		<aim9-signal> 
			<name>aim9-signal</name>
			<mode>looped</mode>
			<path>Aircraft/f-14b/Sounds/115hz-square.wav</path>
			<condition>
				<property>sim/model/f-14b/systems/armament/aim9/sound-on-off</property>
			</condition>
			<volume>
				<property>sim/model/f-14b/systems/armament/aim9/sound-volume</property>
			</volume>
		</aim9-signal>

		<!--ACM jettison lever -->
		<acm-jett-lever-on>
			<name>acm-jett-lever-on</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/shlock.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/armament/acm-jettison-lever</property>
					<value>true</value>
				</equals>
			</condition>
			<volume><factor>0.30</factor></volume>
		</acm-jett-lever-on>
		<acm-jett-lever-off> 
			<name>acm-jett-lever-off</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/armament/acm-jettison-lever</property>
					<value>false</value>
				</equals>
			</condition>
			<volume><factor>0.37</factor></volume>
		</acm-jett-lever-off>

		<!--ACM Master Arm lever -->
		<master-arm-lever-on>
			<name>master-arm-lever-on</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/shlock.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/armament/master-arm-lever</property>
					<value>true</value>
				</equals>
			</condition>
			<volume><factor>0.37</factor></volume>
		</master-arm-lever-on>
		<master-arm-lever-off> 
			<name>master-arm-lever-off</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/armament/master-arm-lever</property>
					<value>false</value>
				</equals>
			</condition>
			<volume><factor>0.42</factor></volume>
		</master-arm-lever-off>

		<!--ACM Master Arm switch -->
		<master-arm-switch-up>
			<name>master-arm-switch-up</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/armament/master-arm-switch</property>
					<value>2</value>
				</equals>
			</condition>
			<volume><factor>0.45</factor></volume>
		</master-arm-switch-up>
		<master-arm-switch-center>
			<name>master-arm-switch-center</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/armament/master-arm-switch</property>
					<value>1</value>
				</equals>
			</condition>
			<volume><factor>0.45</factor></volume>
		</master-arm-switch-center>
		<master-arm-switch-down>
			<name>master-arm-switch-down</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/armament/master-arm-switch</property>
					<value>0</value>
				</equals>
			</condition>
			<volume><factor>0.45</factor></volume>
		</master-arm-switch-down>

		<!-- Warnings tones -->
		<low-alt-warning>
			<name>low-alt-warning</name>
			<path>Aircraft/f-14b/Sounds/low-alt-warning.wav</path>
			<mode>once</mode>
			<condition>
				<less-than>
					<property>position/altitude-agl-ft</property>
					<property>sim/model/f-14b/instrumentation/radar-altimeter/limit-bug</property>
				</less-than>
			</condition>
			<volume><factor>0.25</factor></volume>
		</low-alt-warning>

		<!-- Left Verical console -->
		<refuel-probe-switch-2>
			<name>refuel-probe-switch-2</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/shlock.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/fuel/refuel-probe-switch</property>
					<value>2</value>
				</equals>
			</condition>
			<volume><factor>0.30</factor></volume>
		</refuel-probe-switch-2>
		<refuel-probe-switch-1> 
			<name>refuel-probe-switch-1</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/fuel/refuel-probe-switch</property>
					<value>1</value>
				</equals>
			</condition>
			<volume><factor>0.30</factor></volume>
		</refuel-probe-switch-1>
		<refuel-probe-switch-0> 
			<name>refuel-probe-switch-0</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/fuel/refuel-probe-switch</property>
					<value>0</value>
				</equals>
			</condition>
			<volume><factor>0.50</factor></volume>
		</refuel-probe-switch-0>

		<fuel-qty-sel-switch-up>
			<name>fuel-qty-sel-switch-up</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/shlock.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/fuel/qty-sel-switch</property>
					<value>1</value>
				</equals>
			</condition>
			<volume><factor>0.20</factor></volume>
		</fuel-qty-sel-switch-up>
		<fuel-qty-sel-switch-center> 
			<name>fuel-qty-sel-switch-center</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/fuel/qty-sel-switch</property>
					<value>0</value>
				</equals>
			</condition>
			<volume><factor>0.20</factor></volume>
		</fuel-qty-sel-switch-center>
		<fuel-qty-sel-switch-down> 
			<name>fuel-qty-sel-switch-down</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/fuel/qty-sel-switch</property>
					<value>-1</value>
				</equals>
			</condition>
			<volume><factor>0.20</factor></volume>
		</fuel-qty-sel-switch-down>

		<!-- Master Light Panel -->
		<hook-bypass-switch-on>
			<name>hook-bypass-switch-on</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/lighting/hook-bypass</property>
					<value>true</value>
				</equals>
			</condition>
			<volume><factor>0.25</factor></volume>
		</hook-bypass-switch-on>
		<hook-bypass-switch-off> 
			<name>hook-bypass-switch-off</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/lighting/hook-bypass</property>
					<value>false</value>
				</equals>
			</condition>
			<volume><factor>0.25</factor></volume>
		</hook-bypass-switch-off>

		<!-- Right Verical console -->
		<hsd-mode-switch-up>
			<name>hsd-mode-switch-up</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/pilots-displays/hsd-mode-nav</property>
					<value>-1</value>
				</equals>
			</condition>
			<volume><factor>0.25</factor></volume>
		</hsd-mode-switch-up>
		<hsd-mode-switch-mid> 
			<name>hsd-mode-switch-mid</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/pilots-displays/hsd-mode-nav</property>
					<value>0</value>
				</equals>
			</condition>
			<volume><factor>0.25</factor></volume>
		</hsd-mode-switch-mid>
		<hsd-mode-switch-down> 
			<name>hsd-mode-switch-down</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/pilots-displays/hsd-mode-nav</property>
					<value>1</value>
				</equals>
			</condition>
